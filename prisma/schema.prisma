generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Alumno {
  id_alumno               Int     @id @default(autoincrement())
  run_alumno              String @unique
  nombre_alumno           String
  apellido_paterno_alumno String
  apellido_materno_alumno String
  fono_alumno             Int   @unique
  email_alumno            String  @unique
  password                String?
}
